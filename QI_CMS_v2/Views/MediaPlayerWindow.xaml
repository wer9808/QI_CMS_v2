<Window x:Class="QI_CMS_v2.Views.MediaPlayerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QI_CMS_v2.Views"
        mc:Ignorable="d"
        Title="MediaPlayerWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="MediaPlayerCanvas">
            <Image x:Name="MediaPlayerImage" Visibility="Collapsed" Stretch="Fill">
                
            </Image>
            <MediaElement x:Name="FirstMediaPlayerVideo" Visibility="Collapsed" Stretch="Fill" LoadedBehavior="Manual" Loaded="MediaPlayerVideo_Loaded" UnloadedBehavior="Stop">

            </MediaElement>
            <MediaElement x:Name="SecondMediaPlayerVideo" Visibility="Collapsed" Stretch="Fill" LoadedBehavior="Manual" Loaded="MediaPlayerVideo_Loaded" UnloadedBehavior="Stop">

            </MediaElement>
            <TextBlock x:Name="MediaPlayerText" Visibility="Collapsed">
                
            </TextBlock>
        </Canvas>
    </Grid>
</Window>
